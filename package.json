{
  "name": "gonitely-home-finder",
  "version": "1.0.0",
  "description": "GoNitely Home Finder",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint",
    "build-ts": "node_modules/.bin/tsc",
    "watch-ts": "node_modules/.bin/tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "serve": "node_modules/.bin/nodemon dist/index.js",
    "serve:prod": "node_modules/.bin/pm2 start dist/index.js --no-daemon",
    "tslint": "node_modules/.bin/tslint -c tslint.json -p tsconfig.json",
    "test": "node_modules/.bin/mocha --require ts-node/register tests/**/*.ts"
  },
  "author": "GoNitely",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.5.2",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "cheerio": "^1.0.0-rc.2",
    "commander": "^2.18.0",
    "compression": "^1.7.3",
    "elasticsearch": "^15.1.1",
    "express": "^4.16.3",
    "inversify": "^4.13.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.3.3",
    "numeral": "^2.0.6",
    "pm2": "^3.1.3",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "user-agents": "^1.0.54",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.8",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.5",
    "@types/cheerio": "^0.22.9",
    "@types/compression": "0.0.36",
    "@types/elasticsearch": "^5.0.26",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.2.19",
    "@types/node": "^10.11.3",
    "@types/puppeteer": "^1.9.1",
    "@types/request": "^2.47.1",
    "@types/request-promise": "^4.1.42",
    "@types/uuid": "^3.4.4",
    "concurrently": "^4.0.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "puppeteer": "^1.10.0",
    "puppeteer-extra": "^2.1.1",
    "puppeteer-extra-plugin-anonymize-ua": "^2.1.1",
    "ts-mocha": "^2.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1",
    "xml2json": "^0.11.2"
  }
}
